{
  "name": "comrade-pingu",
  "version": "1.0.0",
  "private": true,
  "publishConfig": {
    "access": "restricted"
  },
  "homepage": "https://comrade-pingu.glitch.me",
  "repository": {
    "type": "git",
    "url": "https://github.com/cherryblossom000/comrade-pingu.git"
  },
  "main": "./dist/src/server.js",
  "engines": {
    "node": ">=11.10.1"
  },
  "scripts": {
    "start": "pnpm run build && node .",
    "test": "eslint . && jest",
    "test:debug": "jest -c ./jest.config.dev.js",
    "build-deps": "pnpm i -P @types/express @types/jest @types/lodash.upperfirst @types/ms @types/node typescript",
    "build:ci": "tsc -p ./tsconfig.build.json",
    "build": "pnpm run build-deps && pnpm run build:ci",
    "watch": "tsc -watch -p ./tsconfig.build.json"
  },
  "dependencies": {
    "discord.js": "^11.6.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "lodash.upperfirst": "^4.3.1",
    "ms": "^2.1.2"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "@types/express": "^4.17.2",
    "@types/jest": "^25.1.3",
    "@types/lodash.upperfirst": "^4.3.6",
    "@types/ms": "^0.7.31",
    "@types/node": "^13.7.7",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^6.8.0",
    "jest": "^25.1.0",
    "jest-mock-console": "^1.0.0",
    "semantic-release": "^17.0.4",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
