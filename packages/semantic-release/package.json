{
  "name": "@comrade-pingu/semantic-release",
  "version": "1.0.2",
  "private": true,
  "publishConfig": {
    "access": "restricted"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cherryblossom000/comrade-pingu",
    "directory": "packages/semantic-release"
  },
  "main": "dist/index",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "lint": "eslint --max-warnings 0 --ext ts .",
    "lint:dev": "pnpm lint -- --cache",
    "typecheck": "tsc -b tsconfig.typecheck.json",
    "build": "tsc -b src",
    "build:dev": "tsc -b src/tsconfig.dev.json",
    "watch": "pnpm build:dev -- -w"
  },
  "dependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/github": "^7.1.1",
    "debug": "^4.2.0",
    "fs-extra": "^9.0.1",
    "lodash": "^4.17.20",
    "mime": "^2.4.6"
  },
  "devDependencies": {
    "@octokit/rest": "^18.0.6",
    "@types/debug": "^4.1.5",
    "@types/fs-extra": "^9.0.1",
    "@types/lodash": "^4.14.161",
    "@types/mime": "^2.0.3",
    "@types/node": "^12.12.62",
    "@types/semantic-release": "^17.1.0",
    "type-fest": "^0.16.0"
  }
}
