{
  "name": "@comrade-pingu/semantic-release",
  "version": "0.0.0-development",
  "private": true,
  "publishConfig": {
    "access": "restricted"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cherryblossom000/comrade-pingu",
    "directory": "packages/semantic-release"
  },
  "main": "dist/index",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "lint": "eslint --report-unused-disable-directives --max-warnings 0 --ext ts .",
    "lint:dev": "pnpm run lint -- --cache",
    "typecheck": "pnpm run build --noEmit",
    "build": "tsc -p src/tsconfig.json",
    "build:dev": "pnpm run build -- --sourceMap --incremental",
    "watch": "pnpm run build:dev -- -w"
  },
  "dependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/github": "^7.0.7",
    "debug": "^4.1.1",
    "fs-extra": "^9.0.1",
    "lodash": "^4.17.19",
    "mime": "^2.4.6"
  },
  "devDependencies": {
    "@octokit/rest": "^18.0.3",
    "@types/debug": "^4.1.5",
    "@types/fs-extra": "^9.0.1",
    "@types/lodash": "^4.14.158",
    "@types/mime": "^2.0.3",
    "@types/node": "^12.12.53",
    "@types/semantic-release": "^17.1.0",
    "type-fest": "^0.16.0"
  }
}
